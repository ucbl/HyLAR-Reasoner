        <rdfs:label xml:lang="it">è introdotto da</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#isLocationOf -->

    <ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#isLocationOf">
